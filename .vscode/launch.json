{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch",
      "type": "cppdbg",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": "build/debug/build/Secure/GTZC_TZSC_MPCBB_TrustZone_Secure.elf",
      "MIMode": "gdb",
      "miDebuggerPath": "${command:vscode-embedded.st.gdb}",
      "miDebuggerServerAddress": "localhost:3333",
      "debugServerPath": "${command:vscode-embedded.st.gdbserver}",
      "debugServerArgs": "--stm32cubeprogrammer-path ${command:vscode-embedded.st.cubeprogrammer} --swd --port-number 3333",
      "serverStarted": "Waiting for connection on port .*\\.\\.\\.",
      "stopAtConnect": true,
      "postRemoteConnectCommands": [
        {
          "text": "load build/debug/build/NonSecure/GTZC_TZSC_MPCBB_TrustZone_NonSecure.elf"
        },
        {
          "text": "load build/debug/build/Secure/GTZC_TZSC_MPCBB_TrustZone_Secure.elf"
        },
        {
          "text": "add-symbol-file build/debug/build/NonSecure/GTZC_TZSC_MPCBB_TrustZone_NonSecure.elf"
        }
      ],
      "logging": {
        "engineLogging": true
      },
      "preLaunchTask": "Build",
      "svdPath": "${command:vscode-embedded.st.svd}/STM32L552.svd"
    }
  ]
}
